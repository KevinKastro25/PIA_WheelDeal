@model IEnumerable<PIA_WheelDeal.Models.dbModels.Venta>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Monto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdIndNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdProdNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <strong>$</strong>@Html.DisplayFor(modelItem => item.Monto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdIndNavigation.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdProdNavigation.IdProd)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdIngreso">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdIngreso">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdIngreso">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
